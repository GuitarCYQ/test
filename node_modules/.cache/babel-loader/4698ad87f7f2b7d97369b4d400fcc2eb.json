   VabProfile,\n      VabCharge,\n    },\n    data() {\n      return {\n        profileShow: true,\n        faultTextShow: true,\n        solidTextShow: true,\n        startVal: 0,\n        endVal: 20,\n        timeInterval: null,\n      }\n    },\n    mounted() {\n      this.handleProfile()\n      this.handleSolidText()\n      this.timeInterval = setInterval(() => {\n        if (this.endVal < 100) {\n          this.startVal = this.endVal\n          this.endVal++\n        }\n      }, 5000)\n    },\n    beforeDestroy() {\n      if (this.clearInterval) {\n        clearInterval(this.timeInterval)\n      }\n    },\n    methods: {\n      handleProfile() {\n        this.profileShow = false\n        setTimeout(() => {\n          this.profileShow = true\n        })\n      },\n      handleSolidText() {\n        this.solidTextShow = false\n        setTimeout(() => {\n          this.solidTextShow = true\n        })\n      },\n    },\n  }\n</script>\n<style lang=\"scss\" scoped>\n  .small-components-container {\n    ::v-deep {\n      .el-card__body {\n        display: flex;\n        align-items: center; /* 垂直居中 */\n        justify-content: center; /* 水平居中 */\n        height: 430px;\n      }\n    }\n  }\n</style>\n"],"sourceRoot":"src/views/vab/smallComponents"}]}