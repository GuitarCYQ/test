SolidText() {\n        this.solidTextShow = false\n        setTimeout(() => {\n          this.solidTextShow = true\n        })\n      },\n    },\n  }\n</script>\n<style lang=\"scss\" scoped>\n  .small-components-container {\n    ::v-deep {\n      .el-card__body {\n        display: flex;\n        align-items: center; /* 垂直居中 */\n        justify-content: center; /* 水平居中 */\n        height: 430px;\n      }\n    }\n  }\n</style>\n"]}]}